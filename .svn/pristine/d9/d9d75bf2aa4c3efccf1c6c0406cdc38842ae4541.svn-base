@import "theme";

@header-h: .9rem;
@year-h: .8rem;
@bottom-h: .98rem;

.hb-ico{
  display: inline-block;
}

/* 头部 */
.hb-header-box{
  width: 100%;
  height: @header-h;
  background-color: @theme-color;
  color: #fff;
  background-color: #08ae9b;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;

  .hb-ico{
    width: .44rem;
    height: .44rem;
    background: url("../images/icon-header.png");
    background-size: .44rem auto;
  }

  .header-left,.header-right{
    position: absolute;
    height: .44rem;
    top: .22rem;
  }
  .header-left{
    left: .24rem;
  }
  .header-right{
    right: .24rem;
  }

  .header-tit{
    text-align: center;
    font-size: .34rem;
    line-height: .9rem;
    width: 58%;
    margin: 0 auto;
    .to;
  }
  .header-txt{
    font-size: .28rem;
    line-height: .9rem;
    color: #fff;
    display: inline-block;
    position: relative;
    top: -.22rem;
  }
  .ico-menu{background-position: 0 0;}
  .ico-center{background-position: 0 -.5rem;}
  .ico-close{background-position: 0 -1rem;}
  .ico-home{background-position: 0 -1.5rem;}
  .ico-return{background-position: 0 -2rem;}
  .ico-save{background-position: 0 -2.5rem;}
}
/* 头部 */

/* 未关联页面 */
.login-img{
  width: 3.12rem;
  height: 3.12rem;
  margin: .4rem 0;
}

.qr-code-box{
  margin-top: .8rem;
  background-color: #f4f4f4;
  padding: .35rem .4rem;
  display: flex;
  align-items: center;

  .qr-code-panel{
    width: 1.76rem;
    height: 1.76rem;
    background-color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
  }

  .qr-code-border{
    width: 1.76rem;
    height: 1.76rem;
    position: absolute;
    left: 0;
    top: 0;
  }
  .qr-code{
    width: 1.6rem;
    height: 1.6rem;
  }

  .info{
    flex: 1;
    text-align: center;

    .tit{
      font-size: .4rem;
    }

    .tips{
      font-size: .26rem;
      color: #999;

      &:before{
        content: "";
        width: 0;
        height: 0;
        border: .1rem solid;
        border-color: transparent @base transparent transparent;
        margin-right: .1rem;
        margin-left: -.2rem;
        display: inline-block;
      }
    }
  }

  .hb-btn{
    font-size: .26rem;
    background-color: @base;
    padding: .1rem .3rem;
    color: #fff;
    display: inline-block;
    border-radius: .6rem;
    margin-top: .2rem;

    &:active{
      background-color: lighten(@base,5%);
    }
  }
}

.qr-code-box-1{
  margin-top: 0;
}

.hb-form-cells{
  padding: 0 .5rem;
  position: relative;
  z-index: 2;

  .tit-box{
    padding: 1rem 0;
  }
  .tit-box-1{
    padding: .7rem 0;
  }
  .h1{
    font-size: .5rem;
  }

  .weui-cells {
    background-color: #FFFFFF;
    line-height: 1.47058824;
    overflow: hidden;
    position: relative;
    &:before{
      border-top: 0;
    }
  }

  .weui-cell {
    padding: .2rem 0;
    position: relative;
    display: flex;
    -webkit-box-align: center;
    align-items: center;

    &:before{
      left: 0;
    }

    &.weui-cell_vcode {
      padding: .16rem 0;
    }

    .weui-vcode-img {
      width: 1.8rem;
      height: .6rem;
      margin-left: .1rem;
      vertical-align: middle;
    }

    .hb-btn-s {
      color: @base;
      width: 1.8rem;
      height: .6rem;
      line-height: 2.3077;
      text-align: center;
      border: .02rem solid @base;
      -webkit-border-radius: .6rem;
      -moz-border-radius: .6rem;
      border-radius: .6rem;
      display: inline-block;
      font-size: .26rem;

      &:active {
        background-color: @base;
        color: #fff;
      }

      &.disabled {
        border-color: #ddd;
        background-color: #eee;
        color: #999;

        &:active {
          background-color: #eee;
          color: #999;
        }
      }
    }
  }
  .weui-label{
    width: 1.2rem;
    font-size: .3rem;
    display: block;
    word-wrap: break-word;
    word-break: break-all;
  }
  .weui-label-1{
    width: 1.7rem;
  }
  .weui-label-2{
    width: 1.4rem;
  }
  .weui-cell__bd {
    flex: 1;
    overflow: hidden;
  }

  .weui-input {
    width: 100%;
    border: 0;
    outline: 0;
    -webkit-appearance: none;
    background-color: transparent;
    font-size: .28rem;
    color: inherit;
    height: 1.47058824em;
    line-height: 1.47058824;
  }


  .form-other{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    font-size: .3rem;
    color: #999;

    .weui-check__label, .a-forget{
      flex: 1;

      &:active{
        text-decoration: underline;
      }
    }
  }

  .hb-panel .weui-cell {
    padding: .2rem 0;
    border-bottom: 0;
  }
}

.login-box-img{
  width: 7.5rem;
  height: 3.8rem;
  position: absolute;
  bottom: 0;
  left: 0;
}

/* 年度选择 */
.hb-year-box{
  width: 100%;
  overflow: hidden;
  height: .8rem;
  line-height: .8rem;
  background-color: #fff;
  z-index: 9;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: .3rem;
  position: fixed;
  top: 0;

  &:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #ddd;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
  }

  .year-tit{
    width: 1rem;
    text-align: center;
    background-color: #f2f2f2;
    margin-right: .1rem;
    position: relative;

    &:after{
      content: "";
      width: 0;
      height: 0;
      border: .12rem solid;
      border-color: transparent transparent transparent #f2f2f2;
      position: absolute;
      right: -.2rem;
      top: 50%;
      margin-top: -.12rem;
    }
  }

  .year-tit-1{
    width: auto;
    padding: 0 .16rem;
    margin-right: .2rem;
  }

  .year-bd{
    white-space: nowrap;
    flex: 1;
    overflow-x: auto;
  }

  .year-bd-1{
    flex: 1;
    display: flex;
    align-items: center;
    .to;
  }

  .items{
    display: inline-block;
    color: #666;
    padding: 0 .2rem;
    position: relative;
    line-height: .8rem;
  }
  .current{
    color: @base;

    &:after{
      content: " ";
      width: .3rem;
      height: 2px;
      border-radius: .02rem;
      background-color: @base;
      position: absolute;
      left: 50%;
      bottom: 0;
      margin-left: -7px;
      display: block;
    }
  }

  .change-label{
    width: .8rem;
    height: .8rem;
    position: relative;
    text-align: center;

    &:after {
      content: " ";
      position: absolute;
      left: 0;
      bottom: 0;
      top: -.8rem;
      width: 1px;
      border-left: 1px solid #ddd;
      -webkit-transform-origin: 0 100%;
      transform-origin: 0 100%;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
    }

    &.open{
      .arrow{
        border-width: .03rem 0 0 .03rem;
        top: .1rem;
      }
    }

    .arrow{
      display: inline-block;
      height: .26rem;
      width: .26rem;
      border-width: 0 .03rem .03rem 0;
      border-color: @base;
      border-style: solid;
      -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      position: relative;
      top: -.01rem;
    }

    .dot{
      width: .12rem;
      height: .12rem;
      border-radius: .12rem;
      background-color: #ed2424;
      display: inline-block;
      position: absolute;
      top: .16rem;
      right: .1rem;
    }

  }
  .change-label-2{
    width: .8rem;
    height: .8rem;
    position: relative;
    text-align: center;

    &:after {
      content: " ";
      position: absolute;
      left: 0;
      bottom: 0;
      top: -.8rem;
      width: 1px;
      border-left: 1px solid #ddd;
      -webkit-transform-origin: 0 100%;
      transform-origin: 0 100%;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
    }

    .arrow{
      display: inline-block;
      height: .26rem;
      width: .26rem;
      border-width: .03rem .03rem  0 0;
      border-color: #ccc;
      border-style: solid;
      -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      position: relative;
      top: 0;
    }

  }
  .change-label-3{
    width: .6rem;
    height: .8rem;
    position: relative;
    text-align: center;

    .arrow{
      display: inline-block;
      height: .26rem;
      width: .26rem;
      border-width: 0 0 .03rem .03rem;
      border-color: #ccc;
      border-style: solid;
      -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      position: relative;
      right: -.05rem;
    }
  }
  .change-label-4{
    width: .6rem;
    height: .8rem;
    position: relative;
    text-align: center;

    .arrow{
      display: inline-block;
      height: .26rem;
      width: .26rem;
      border-width: .03rem .03rem  0 0;
      border-color: #ccc;
      border-style: solid;
      -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
      position: relative;
      left: -.05rem;
    }

  }
  .warning-bg{
    color: #ed2424;
    background-color: #fbe0df;
    padding: 0 .2rem;
    border-radius: .4rem;
    display: inline-block;
    max-width: 98%;
    line-height: .52rem;
    .to;
  }
}
.hb-year-box-1{
  top: .9rem;
}
.hb-year-box-2,.hb-year-box-3{
  .items{
    max-width: 2.5rem;
    padding: .05rem .2rem;
    border-radius: .3rem;
    border: .02rem solid #ddd;
    line-height: 1.5;
