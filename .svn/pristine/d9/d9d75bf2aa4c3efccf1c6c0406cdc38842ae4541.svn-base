/* 侧边栏 */
.hb-popover{
  position: fixed;
  top: @header-h;
  left: .1rem;
  z-index: 1000;
  width: 3.6rem;

  .popover-arrow{
    position: absolute;
    width: 0;
    height: 0;
    border-left: 8px solid transparent;
    border-right: 8px solid transparent;
    border-bottom: 8px solid #38373d;
    left: .36rem;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
    top: -.12rem;
  }
  .popover-bd{
    background-color: #38373d;
    border-radius: .1rem;
  }
  .user-img-box{
    text-align: center;
    padding-top: .4rem;
    padding-bottom: .2rem;

    .user-img{
      width: 1.4rem;
      height: 1.4rem;
      border: .04rem solid #fff;
      border-radius: 1.4rem;
    }

    .hd-btn{
      display: block;
      width: 1.4rem;
      height: .6rem;
      line-height: .6rem;
      margin: .2rem auto;
      padding: 0;
      border-radius: .3rem;
      color: #fff;
      background-color: transparent;
      border: .02rem solid #fff;
    }
  }
  .hb-menu-box{
    padding-bottom: .4rem;
    .menu-items{
      height: .9rem;
      line-height: .9rem;
      color: #bbb;
      font-size: .3rem;
      display: block;
      position: relative;

      .hb-ico{
        width: .38rem;
        height: .38rem;
        background: url("../images/icon-menu.png");
        background-size: .38rem;
        margin: 0 .24rem 0 .5rem;
        position: relative;
        top: .08rem;
      }
      .ico-home{background-position: 0 0;}
      .ico-list{background-position: 0 -.4rem;}
      .ico-class{background-position: 0 -.8rem;}
      .ico-prove{background-position: 0 -1.2rem;}
      .ico-app{background-position: 0 -1.6rem;}
      .ico-exit{background-position: 0 -2rem;}
      .ico-found{background-position: 0 -4.8rem;}
      .ico-car{background-position: 0 -5.6rem;}

      &:active, &.current{
        background-color: #525155;
        color: #fff;

        &:before{
          content: " ";
          position: absolute;
          left: 0;
          top: 0;
          height: 100%;
          width: .06rem;
          background-color: @theme-color;
        }

        .ico-home{background-position: 0 -2.4rem;}
        .ico-list{background-position: 0 -2.8rem;}
        .ico-class{background-position: 0 -3.2rem;}
        .ico-prove{background-position: 0 -3.6rem;}
        .ico-app{background-position: 0 -4rem;}
        .ico-exit{background-position: 0 -4.4rem;}
        .ico-found{background-position: 0 -5.2rem;}
        .ico-car{background-position: 0 -6rem;}
      }
    }
  }
}

