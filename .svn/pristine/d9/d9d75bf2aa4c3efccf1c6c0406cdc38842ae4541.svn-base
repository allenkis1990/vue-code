/* 返回顶部 */
.layer-box{
  position: fixed;
  bottom: 1.2rem;
  right: .4rem;
  z-index: 99;

  .a-items{
    display: block;
    margin-top: .1rem;

    .img{
      display: block;
      width: .8rem;
      height: .8rem;
    }
  }
}

.hb-tab-tit{
  height: .9rem;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: @header-h;
  width: 100%;
  z-index: 999;


  .tab-tit-items{
    flex: 1;
    text-align: center;
    height: 100%;
    position: relative;
    overflow: hidden;

    .items-name{
      font-size: .32rem;
      height: 100%;
      line-height: 2.8125;
      color: #666;
      display: block;
    }

    &:after {
      content: " ";
      position: absolute;
      left: 0;
      bottom: 0;
      right: 0;
      height: 1px;
      border-bottom: 1px solid #ddd;
      -webkit-transform-origin: 0 100%;
      transform-origin: 0 100%;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
    }

    &.current{
      .items-name{
        color: @base;
      }

      .current-img{
        display: block;
      }

      &:after {
        border-bottom: 0;
      }
    }
  }

  .current-img{
    width: 3.75rem;
    height: .14rem;
    display: none;
    position: absolute;
    bottom: 0;
    left: 50%;
    margin-left: -1.86rem;
  }
}

.hb-tab-tit-1{
  top: 4.22rem;
}
.hb-tab-tit-2{
  top: 3rem;
}
.screening-bar{
  width: 100%;
  height: .9rem;
  border-bottom: .02rem solid #ddd;
  padding-left: .3rem;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: .9rem + @header-h;
  background-color: #fff;
  z-index: 999;
  box-sizing: border-box;

  .screening-items{
    flex: 1;
    height: 100%;
  }
  .items-tit{
    margin-right: .3rem;
    margin-top: .18rem;
    height: .54rem;
    font-size: .28rem;
    color: #888;
    background-color: #f2f2f2;
    border-radius: .1rem;
    text-align: center;
    line-height: 1.9286;
    max-width: 3.3rem;
    position: relative;

    &:after{
      content: "";
      width: 0;
      height: 0;
      border: .1rem solid;
      border-color: #888 transparent transparent transparent;
      margin-left: .1rem;
      display: inline-block;
      position: relative;
      top: .06rem;
    }
  }

  .selected{
    .items-tit{
      background-color: #fff;
      border: .02rem solid @base;
      color: @base;
      line-height: 1.8571;

      &:after{
        border-color: @base transparent transparent transparent;
      }
    }
  }

  .open{
    .items-tit{
      background-color: #fff;
      border: .02rem solid #ddd;
      color: #888;
      line-height: 1.8571;
      height: .8rem;
      border-bottom-color: #fff;
      border-radius: .1rem .1rem 0 0;

      &:after{
        border-color: transparent transparent #888 transparent;
        top: -.06rem;
      }
    }
  }

  .select-items-box{
    width: 100%;
    position: absolute;
    top: .9rem;
    bottom: 0;
    left: 0;
    z-index: 999;

    .mask{
      width: 100%;
      height: 100%;
      position: fixed;
      top: 1.8rem + @header-h;
      bottom: 0;
      background-color: rgba(0,0,0,.5);
    }
    .box-bd{
      width: 100%;
      background-color: #fff;
      padding: .4rem .4rem 0;
      position: relative;
      z-index: 1;
      max-height: 5.6rem;
      overflow: auto;
    }

    .select-item{
      color: #666;
      font-size: .3rem;
      padding-bottom: .4rem;
      padding-left: .32rem;
      position: relative;

      &.current{
        color: @base;

        &:before{
          content: "";
          width: 0;
          height: 0;
          border: .12rem solid;
          border-color: transparent transparent transparent @base;
          display: block;
          position: absolute;
          top: .1rem;
          left: 0;
        }
      }
    }

    .btn-primary{
      position: relative;
      z-index: 1;
      border-radius: 0;
    }
  }
}
.screening-bar-1{
  top: .9rem;
}
.screening-bar-2{
